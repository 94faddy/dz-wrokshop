{
  "name": "dz-workshop-downloader",
  "version": "2.0.0",
  "description": "DayZ Workshop Mod Downloader with SteamCMD - Enhanced for Large Files + Admin Dashboard",
  "main": "server.js",
  "scripts": {
    "dev": "next dev -p 3020",
    "build": "next build",
    "start": "next start -p 3020",
    "server": "node server.js",
    "dev:server": "node server.js",
    "dev:all": "npx concurrently \"npm run dev\" \"npm run dev:server\"",
    "dev:both": "npx concurrently -n \"FRONTEND,BACKEND\" -c \"cyan,yellow\" \"npm run dev\" \"npm run dev:server\"",
    "prod:all": "npx concurrently \"npm run start\" \"npm run server\"",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "hash-password": "node utils/hashPassword.js",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "next lint",
    "clean": "rm -rf .next node_modules/.cache",
    "setup": "npm install && npm run build",
    "install-admin": "npm install bcryptjs geoip-lite useragent uuid moment csv-parser json2csv@5.0.7 jsonwebtoken",
    "setup-admin": "npm run install-admin && node utils/hashPassword.js"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "geoip-lite": "^1.4.10",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.2",
    "next": "^14.0.4",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sequelize": "^6.37.7",
    "stream": "^0.0.2",
    "useragent": "^2.3.0",
    "util": "^0.12.5",
    "uuid": "^9.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "dayz",
    "steam",
    "workshop",
    "downloader",
    "steamcmd",
    "large-files",
    "zip",
    "archiver",
    "admin",
    "dashboard",
    "analytics",
    "monitoring"
  ],
  "author": "CRYTEKSOFT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/dayz-workshop-downloader.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/dayz-workshop-downloader/issues"
  },
  "homepage": "https://dayzworkshop.linkz.ltd"
}
